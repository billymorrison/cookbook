    Serves\n          <StyledInput\n            type=\"number\"\n            required\n            value={recipe.serves}\n            onChange={handleRecipeChange}\n            name=\"serves\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Prep Time\n          <StyledInput\n            type=\"text\"\n            required\n            onChange={handleRecipeChange}\n            name=\"prepTime\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Ingredients\n          <DoubleInput>\n            <StyledLabel>\n              Ingredient\n              <IngredientInput\n                type=\"text\"\n                required\n                value={recipe.ingredients}\n                onChange={handleRecipeChange}\n                name=\"ingredientKey\"\n              />\n            </StyledLabel>\n            <StyledLabel>\n              Quantity\n              <IngredientInput\n                type=\"text\"\n                required\n                value={recipe.quantity}\n                onChange={handleRecipeChange}\n                name=\"ingredientValue\"\n              />\n            </StyledLabel>\n          </DoubleInput>\n        </StyledLabel>\n        <StyledLabel>\n          Method\n          <StyledInput\n            type=\"text\"\n            required\n            value={recipe.method}\n            onChange={handleRecipeChange}\n            name=\"method\"\n          />\n        </StyledLabel>\n        <StyledButton />\n      </StyledForm>\n    </>\n  );\n};\n\nexport default AddRecipe;\n","/Users/benthompson/Projects/cookbook/src/components/Contact.js",[],"/Users/benthompson/Projects/cookbook/src/components/Home.js",[],"/Users/benthompson/Projects/cookbook/src/components/GlobalStyles.js",[],"/Users/benthompson/Projects/cookbook/src/components/Alert.js",[],{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","severity":1,"message":"63","line":1,"column":8,"nodeType":"64","message
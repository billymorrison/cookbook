) => {\n    const formData = new FormData();\n    for(const key in data) {\n      formData.set(key, data[key])\n    }\n    return formData\n  }\n\n  const handleAddRecipe = async (event) => {\n    event.preventDefault();\n    \n\n    const formData = setFormData(recipe)\n    console.log(formData)\n    setAlert({ message: \"\", isSuccess: false });\n\n    await axios.post(\"http://localhost:3000/recipes\", formData)\n      .then(() => {\n        setAlert({\n          message: \"Recipe Added\",\n          isSuccess: true,\n        });\n      })\n      .catch(() =>\n        setAlert({\n          message: \"Server error. Please try again later.\",\n          isSuccess: false,\n        })\n      );\n  };\n  return (\n    <>\n      <StyledAlert message={alert.message} success={alert.isSuccess} />\n      <AddRecipeForm onSubmit={handleAddRecipe} encType=\"multipart/form-data\">\n        <StyledLabel>\n          Title\n          <StyledInput\n            type=\"text\"\n            required\n            value={recipe.title}\n            onChange={handleRecipeChange}\n            name=\"title\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Cook Time\n          <StyledInput\n            type=\"text\"\n            value={recipe.cookTime}\n            onChange={handleRecipeChange}\n            name=\"cookTime\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Difficulty\n          <StyledSelect\n            name=\"difficulty\"\n            value={recipe.difficulty}\n            onChange={handleRecipeChange}\n          >\n            <option value=\"easy\">Easy</option>\n            <option value=\"medium\">Medium</option>\n            <option value=\"hard\">Hard</option>\n          </StyledSelect>\n        </StyledLabel>\n        <StyledLabel>\n          Serves\n          <StyledInput\n            type=\"number\"\n            required\n            value={recipe.serves}\n            onChange={handleRecipeChange}\n            name=\"serves\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Prep Time\n          <StyledInput\n            type=\"text\"\n            required\n            onChange={handleRecipeChange}\n            name=\"prepTime\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Ingredients\n          <StyledTextArea \n            type=\"text\"\n            required\n            onChange={handleRecipeChange}\n            name=\"ingredients\"\n          />\n        </StyledLabel>\n        <StyledLabel>\n          Method\n          <StyledTextArea\n            type=\"text\"\n            required\n            value={recipe.method}\n            onChange={handleRecipeChange}\n            name=\"method\"\n          />\n          <StyledLabel>\n          Image\n          <StyledInput\n            type=\"file\"\n            required\n            onChange={handleFileChange}\n            name=\"image\"\n          />\n        </StyledLabel>\n        </StyledLabel>\n        <StyledButton>Submit</StyledButton>\n      </AddRecipeForm>\n    </>\n  );\n};\n\nexport default AddRecipe;\n","/Users/billymorrison/Documents/Projects/cookbook/src/components/Nav.js",[],"/Users/billymorrison/Documents/Projects/cookbook/src/components/AllRecipes.js",[],"/Users/billymorrison/Documents/Projects/cookbook/src/components/Recipe.js",[],"/Users/billymorrison/Documents/Projects/cookbook/src/components/GlobalStyles.js",[],"/Users/billymorrison/Documents/Projects/cookbook/src/components/Alert.js",[],"/Users/billymorrison/Documents/Projects/cookbook/src/components/RecipeCard.js",[],["71","72"],"/Users/billymorrison/Documents/Projects/cookbook/src/components/UserRecipes.js",["73"],"import React, {useState, useEffect} from \"react\";\nimport RecipeCard from \"./RecipeCard\";\nimport Alert from \"./Alert\";\nimport styled from \"styled-components\";\nimport axios from \"axios\"\n\nconst RecipesGrid = styled.div`\n  display: grid;\n  grid-template-columns: 1fr 1fr 1fr 1fr;\n 